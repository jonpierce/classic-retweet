{
  "manifest_version": 2,
  "name": "Classic Retweet",
  "version": "1.7",
  "description": "Old school retweets for #NewTwitter.",
  "icons": { "16": "icon-16.png",
             "48": "icon-48.png",
            "128": "icon-128.png" },
  "content_scripts": [
    {
      "matches": ["http://twitter.com/*", "https://twitter.com/*"],
      "js": ["main.js"]
    }
  ],
  "web_accessible_resources": [
    "classic-retweet.js"
  ]
}